#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main(void) {
	int a, b, c;
	scanf_s("%d %d %d", &a, &b, &c);
	if (a && b && c > 2) {
		printf("%d\n", (a + b) % c);
		printf("%d\n", ((a % c) + (b % c)) % c);
		printf("%d\n", (a * b) % c);
		printf("%d\n", ((a % c) * (b % c)) % c);

		return 0;
	}

}
